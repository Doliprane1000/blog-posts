  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "clean": "echo 'Removing dist files...' && rm -rf build/* src/generator/tmp/*",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production node ./src/index.js",
    "start": "NODE_ENV=development node ./src/index.js",
    "lighthouse": "lighthouse http://localhost:8080/",
    "prettier": "echo 'Prettifying code...' && prettier --write \"src/**/*.js\" > /dev/null",
    "prelint": "npm run -s clean && npm -s run prettier",
    "lint": "eslint \"src/**/*.js\""
  },
  "dependencies": {
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "lint-staged": "^4.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-async-bootstrapper": "^1.1.1",
    "react-async-component": "^1.0.0-beta.3",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-server": "^4.2.1",
    "react-svg-inline": "^2.0.0",
    "serialize-javascript": "^1.3.0",
    "sitemap": "^1.12.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "fs": "^0.0.1-security",
    "husky": "^0.13.4",
    "lighthouse": "^2.1.0",
    "markdown-it": "^8.3.1",
    "markdown-it-anchor": "^4.0.0",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "prettier": "^1.4.4",
    "prismjs": "^1.6.0",
    "raw-loader": "^0.5.1",
    "rss": "^1.2.2",
    "rxjs": "^5.4.1",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  }
}
